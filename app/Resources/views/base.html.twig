<!DOCTYPE html>
<html>
    <head>
    	<meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        
        <!-- CSS -->
		<link rel="stylesheet" type="text/css" href="{{ asset('bundles/freebeatshome/css/site_global.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('bundles/freebeatshome/css/master_a-gabarit.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('bundles/freebeatshome/css/index.css') }}">		
		<link rel="stylesheet" type="text/css" href="{{ asset('bundles/freebeatshome/css/search.css') }}">		
		<link rel="stylesheet" type="text/css" href="{{ asset('bundles/freebeatshome/css/footer.css') }}">
		
		<!-- Other scripts -->
  		<script type="text/javascript">document.documentElement.className = 'js';</script>
    	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
      <div class="shadow rounded-corners clearfix" id="page"><!-- column -->
	   <div class="position_content" id="page_position_content">
	    <div class="clearfix colelem" id="pu329"><!-- group -->
	        {% block header %}{% include '::header.html.twig' %}{% endblock %}
	        
	        <div class="content grpelem">
	        	{% block content %}{% include '::content.html.twig' %}{% endblock %}
	        </div>
	        
	       	{% block footer %}{% include '::footer.html.twig' %}{% endblock %}
	       	        
	       	{% block javascripts %}
		        
		        <!-- JS includes -->
				<script type="text/javascript">
				 if (document.location.protocol != 'https:') document.write('\x3Cscript src="http://musecdn.businesscatalyst.com/scripts/1.1/jquery-1.7.min.js" type="text/javascript">\x3C/script>');
				</script>
				<script src="http://musecdn.businesscatalyst.com/scripts/1.1/jquery-1.7.min.js" type="text/javascript"></script>
				<script type="text/javascript">
				   window.jQuery || document.write('\x3Cscript src="{{ asset("bundles/freebeatshome/js/jquery-1.7.min.js") }}" type="text/javascript">\x3C/script>');
				</script>
				<script src="{{ asset('bundles/freebeatshome/js/museutils.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/jquery.musemenu.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/webpro.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/musewpslideshow.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/jquery.museoverlay.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/jquery.tobrowserwidth.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/musewpdisclosure.js') }}" type="text/javascript"></script>
				<script src="{{ asset('bundles/freebeatshome/js/jquery.pie.js') }}" type="text/javascript"></script>
				<!-- Other scripts -->
				<script type="text/javascript">
				  	Muse.Utils.addSelectorFn('body', Muse.Utils.transformMarkupToFixBrowserProblemsPreInit);/* body */
					$(document).ready(function() { $('.browser_width').toBrowserWidth(); });/* browser width elements */
					Muse.Utils.addSelectorFn('a.nonblock', Muse.Utils.addHyperlinkAnchor); /* a.nonblock */
					Muse.Utils.addSelectorFn('.MenuBar', function(elem) { return $(elem).museMenu(); });/* unifiedNavBar */
					Muse.Utils.addSelectorFn('a.block', Muse.Utils.addHyperlinkBlock); /* a.block */
					Muse.Utils.addSelectorFn('body', Muse.Utils.showWidgetsWhenReady);/* body */
					Muse.Utils.addSelectorFn('body', Muse.Utils.transformMarkupToFixBrowserProblems);/* body */
				</script>
				
			{% endblock %}
    </body>
</html>